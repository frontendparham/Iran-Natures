{
  "name": "iran-natours",
  "version": "1.0.0",
  "description": "Landing page for Iran Nature's Tours",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss dist/css/style.css -w",
    "devserver": "live-server dist",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass sass/main.scss dist/css/style.comp.css",
    "concat:css": "concat -o dist/css/style.concat.css dist/css/font.css dist/css/icon-font.css dist/css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' dist/css/style.concat.css -o dist/css/style.prefix.css",
    "compress:css": "node-sass dist/css/style.prefix.css dist/css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass concat:css prefix:css compress:css",
    "deploy": "gh-pages -d dist"
  },
  "author": "Parham",
  "license": "ISC",
  "homepage": "https://frontendparham.github.io/Iran-Natures",
  "devDependencies": {
    "autoprefixer": "^10.4.0",
    "concat": "^1.0.3",
    "node-sass": "^6.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.11",
    "postcss-cli": "^9.0.2"
  },
  "dependencies": {
    "gh-pages": "^3.2.3"
  }
}
